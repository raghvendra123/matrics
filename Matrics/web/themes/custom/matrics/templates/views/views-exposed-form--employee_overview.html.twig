{#
/**
 * @file
 * Theme override for a views exposed form.
 *
 * Available variables:
 * - form: A render element representing the form.
 *
 * @see template_preprocess_views_exposed_form()
 */
#}
{% if q is not empty %}
  {#
    This ensures that, if clean URLs are off, the 'q' is added first,
    as a hidden form element, so that it shows up first in the POST URL.
  #}
{{ q }}
{% endif %}
<div class="filter-mobile-head"><h2>Filter</h2></div>
<div class="page-topForm page-topForm-mobile">
  <div class="row">
    <div class="col-xl-9">
      <div class="row inner_top_row">
        <div class="col-lg-4 form_col_asset">
          <div class="form-group focus_input">
            {{ form.field_asset_target_id_entityreference_filter }}
          </div>
        </div>
        <div class="col-lg-4 form_col_asset">
          <div class="form-group focus_input">
            {{ form.field_location_country_code }}
          </div>
        </div>
        <div class="col-lg-4 form_col_asset">
          <div class="form-group focus_input">
            {#{{ form.field_job_title_target_id }}#}
            {{ form.field_job_title_value }}
          </div>
        </div>
        <div class="col-lg-4 form_col_asset">
          <div class="form-group focus_input">
            {#{{ form.field_job_title_target_id }}#}
            {{ form.field_training_gap_count_value }}
          </div>
        </div>
        <div class="col-lg-4 form_col_asset">
          <div class="form-group focus_input">
            {#{{ form.field_job_title_target_id }}#}
            {{ form.field_training_gap_score_value }}
          </div>
        </div>
        <div class="col-lg-4 form_col_asset" style="display: none;">
          <div class="form-group focus_input">
            {#{{ form.field_job_title_target_id }}#}
            {{ form.field_customer_target_id_1 }}
          </div>
        </div>
      </div>
    </div>
    <div class="col-xl-3">
      <div class="row inner_top_row">
        <div class="col-lg-12 ms-lg-auto search_col">
          <div class="form-group focus_input">
            {{ form.field_employee_name_value }}
            {{ form.form_build_id }}
            {{ form.form_id }}
            {{ form.actions }}                       
          </div>
        </div>
      </div>
    </div>
  </div>
</div>